<included>
    <include resource="com/bkahlert/kommons/logging/logback/defaults.xml"/>
    <property name="LOG_FILE" value="${LOG_FILE:-${LOG_PATH:-${LOG_TEMP:-${java.io.tmpdir:-/tmp}}}/kommons.log}"/>
    <include resource="com/bkahlert/kommons/logging/logback/appenders/console.${CONSOLE_LOG_PRESET}-preset.xml"/>
    <include resource="com/bkahlert/kommons/logging/logback/appenders/file.${FILE_LOG_PRESET}-preset.xml"/>
    <root level="INFO">
        <appender-ref ref="CONSOLE"/>
        <appender-ref ref="FILE"/>
    </root>
</included>
