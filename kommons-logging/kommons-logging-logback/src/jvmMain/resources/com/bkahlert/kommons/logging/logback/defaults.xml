    <property scope="context" name="CONSOLE_LOG_PRESET" value="${CONSOLE_LOG_PRESET:-spring}"/>
    <property scope="context" name="FILE_LOG_PRESET" value="${FILE_LOG_PRESET:-off}"/>
</included>
